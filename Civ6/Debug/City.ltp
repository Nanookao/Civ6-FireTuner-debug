    <SelectionListData>
      <Name>City Property;Value</Name>
      <Sorted>false</Sorted>

      <Location>
        <X>700</X>
        <Y>180</Y>
      </Location>
      <Size>
        <Width>180</Width>
        <Height>150</Height>
